{
  "name": "@layerzerolabs/lz-evm-protocol-v2",
  "version": "1.5.20",
  "license": "BUSL-1.1",
  "files": [
    "contracts/**/*"
  ],
  "scripts": {
    "build": "forge build && $npm_execpath build-hardhat",
    "build-hardhat": "$npm_execpath hardhat compile",
    "build-ts": "$npm_execpath tsc -b",
    "clean": "rimraf .turbo && rimraf cache && rimraf out && rimraf node_modules && rimraf hh-cache && rimraf artifacts && rimraf typechain-types && rimraf src/typechain-types && rimraf dist",
    "coverage": "forge coverage",
    "coverage-report": "forge coverage --report lcov",
    "format": "prettier --ignore-path $(git rev-parse --show-toplevel)/.prettierignore --plugin=prettier-plugin-solidity --write .",
    "lint": "solhint ./contracts/**/*.sol --fix",
    "test": "forge test",
    "test-static-analyze": "$npm_execpath hardhat clean && $npm_execpath build-hardhat && slither . --ignore-compile"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.1",
    "solidity-bytes-utils": "0.8.0"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@layerzerolabs-internal/typescript-config": "^1.5.20",
    "@layerzerolabs/hardhat-collect-outcomes": "^1.5.20",
    "@layerzerolabs/hardhat-config": "^1.5.20",
    "@layerzerolabs/lz-definitions": "^1.5.20",
    "@matterlabs/hardhat-zksync-deploy": "^0.6.3",
    "@matterlabs/hardhat-zksync-solc": "^0.3.14",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/node": "^18.15.11",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.16.0",
    "hardhat-deploy": "^0.11.22",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "prettier": "^2.8.4",
    "prettier-plugin-solidity": "^1.1.2",
    "rimraf": "^4.3.0",
    "solhint": "^3.4.0",
    "typechain": "^8.1.1",
    "typescript": "^5.1.3",
    "zksync-web3": "^0.13.1"
  },
  "publishConfig": {
    "access": "public"
  }
}