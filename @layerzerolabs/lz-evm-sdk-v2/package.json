{
  "name": "@layerzerolabs/lz-evm-sdk-v2",
  "version": "1.5.27",
  "license": "BUSL-1.1",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./artifacts/*": "./artifacts/*",
    "./deployments/*": "./deployments/*"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "artifacts/contracts/**/*",
    "deployments/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "$npm_execpath typechain && $npm_execpath tsup",
    "clean": "rimraf .turbo && rimraf artifacts && rimraf dist && rimraf chaintypes",
    "typechain": "typechain --target ethers-v5 'artifacts/contracts/**/*.json' --out-dir chaintypes"
  },
  "dependencies": {
    "ethers": "^5.7.2"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@layerzerolabs-internal/tsup-config": "^1.5.27",
    "@layerzerolabs-internal/typescript-config": "^1.5.27",
    "@layerzerolabs/hardhat-collect-outcomes": "^1.5.27",
    "@layerzerolabs/hardhat-config": "^1.5.27",
    "@layerzerolabs/lz-definitions": "^1.5.27",
    "@layerzerolabs/lz-evm-messagelib-v2": "^1.5.27",
    "@layerzerolabs/lz-evm-oapp-v2": "^1.5.27",
    "@layerzerolabs/lz-evm-protocol-v2": "^1.5.27",
    "@layerzerolabs/monorepo-utilities": "^1.5.27",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/glob": "^8.1.0",
    "@types/node": "^18.15.11",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.16.0",
    "hardhat-deploy": "^0.11.22",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "prettier": "^2.8.4",
    "prettier-plugin-solidity": "^1.1.2",
    "rimraf": "^4.3.0",
    "solhint": "^3.4.0",
    "tsup": "^6.7.0",
    "typechain": "^8.1.1",
    "typescript": "^5.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}