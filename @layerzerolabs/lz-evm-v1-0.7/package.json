{
  "name": "@layerzerolabs/lz-evm-v1-0.7",
  "version": "1.5.31",
  "license": "BUSL-1.1",
  "main": "dist/index.js",
  "files": [
    "contracts/interfaces/**/*",
    "contracts/precrime/interfaces/**/*",
    "contracts/precrime/PreCrimeBase.sol",
    "contracts/precrime/PreCrimeView.sol"
  ],
  "scripts": {
    "build": "$npm_execpath build-forge && $npm_execpath build-hardhat && $npm_execpath build-ts",
    "build-forge": "forge build",
    "build-hardhat": "$npm_execpath hardhat compile",
    "build-ts": "$npm_execpath tsc -b",
    "clean": "$npm_execpath rimraf .turbo && $npm_execpath clean-hardhat && $npm_execpath rimraf dist",
    "clean-hardhat": "$npm_execpath rimraf '*artifacts' && $npm_execpath rimraf '*cache'",
    "format": "$npm_execpath prettier --ignore-path $(git rev-parse --show-toplevel)/.prettierignore --write ",
    "lint": "TIMING=1 $npm_execpath eslint --no-error-on-unmatched-pattern ./**/*.ts --fix && solhint ./contracts/**/*.sol --fix",
    "test": "$npm_execpath test-forge && $npm_execpath hardhat test",
    "test-forge": "forge test"
  },
  "resolutions": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers^v0.3.0-beta.14"
  },
  "dependencies": {
    "@openzeppelin/contracts": "3.4.2-solc-0.7",
    "@openzeppelin/contracts-upgradeable": "3.4.2-solc-0.7"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@layerzerolabs-internal/typescript-config": "^1.5.31",
    "@layerzerolabs/hardhat-collect-outcomes": "^1.5.31",
    "@layerzerolabs/hardhat-config": "^1.5.31",
    "@layerzerolabs/lz-definitions": "^1.5.31",
    "@layerzerolabs/lz-proof-utility": "^1.5.31",
    "@matterlabs/hardhat-zksync-deploy": "^0.6.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.5",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.4.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.1",
    "abi-decoder": "^2.4.0",
    "chai": "^4.3.7",
    "chai-ethers": "^0.0.1",
    "cross-cat": "^1.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.16.0",
    "hardhat-contract-sizer": "^2.8.0",
    "hardhat-deploy": "^0.11.22",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "mocha": "^10.2.0",
    "pino": "^8.8.0",
    "rimraf": "^3.0.2",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.2",
    "tiny-invariant": "^1.3.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.1.3",
    "web3": "^1.8.2"
  }
}