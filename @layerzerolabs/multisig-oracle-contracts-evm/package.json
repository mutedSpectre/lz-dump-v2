{
  "name": "@layerzerolabs/multisig-oracle-contracts-evm",
  "version": "1.5.31",
  "license": "BUSL-1.1",
  "main": "dist/index.js",
  "files": [
    "deployments/**/*",
    "artifacts/contracts/AccessControl.sol/AccessControl.json",
    "artifacts/contracts/MultiSig.sol/MultiSig.json",
    "artifacts/contracts/MultiSigOracle.sol/MultiSigOracle.json",
    "dist/**/*"
  ],
  "scripts": {
    "build": "$npm_execpath build-hardhat && $npm_execpath build-zksync && $npm_execpath build-ts",
    "build-hardhat": "$npm_execpath hardhat compile",
    "build-ts": "$npm_execpath tsc -b",
    "build-zksync": "$npm_execpath hardhat --network zksync-mainnet compile",
    "clean": "$npm_execpath rimraf .turbo && $npm_execpath clean-hardhat && $npm_execpath clean-ts",
    "clean-hardhat": "$npm_execpath rimraf '*artifacts*' && $npm_execpath rimraf '*cache*'",
    "clean-ts": "$npm_execpath rimraf 'dist'",
    "format": "$npm_execpath prettier --ignore-path $(git rev-parse --show-toplevel)/.prettierignore --write ",
    "lint": "$npm_execpath eslint --no-error-on-unmatched-pattern  --ext .ts --fix",
    "test": "$npm_execpath hardhat test"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/web": "^5.7.1",
    "@layerzerolabs-internal/typescript-config": "^1.5.31",
    "@layerzerolabs/hardhat-config": "^1.5.31",
    "@layerzerolabs/lz-definitions": "^1.5.31",
    "@layerzerolabs/lz-evm-messagelib-v2": "^1.5.31",
    "@layerzerolabs/lz-evm-sdk-v1": "^1.5.31",
    "@layerzerolabs/lz-evm-sdk-v2": "^1.5.31",
    "@layerzerolabs/lz-evm-v1-0.7": "^1.5.31",
    "@layerzerolabs/lz-evm-v1-0.8": "^1.5.31",
    "@matterlabs/hardhat-zksync-deploy": "^0.6.3",
    "@matterlabs/hardhat-zksync-solc": "^0.3.14",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
    "@nomicfoundation/hardhat-toolbox": "^2.0.1",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.5",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "3.4.2-solc-0.7",
    "@openzeppelin/contracts-upgradeable": "3.4.2-solc-0.7",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/node": "^18.14.6",
    "abi-decoder": "^2.4.0",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "ethers": "^5.7.2",
    "glob": "^10.2.2",
    "hardhat": "^2.16.0",
    "hardhat-deploy": "^0.11.22",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "mocha": "^10.2.0",
    "pino": "^8.8.0",
    "rimraf": "^3.0.2",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.2",
    "tiny-invariant": "^1.3.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.1.3",
    "web3": "^1.8.2",
    "zksync-web3": "^0.13.1"
  }
}